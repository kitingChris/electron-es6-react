{
  "name": "electron-es6-react",
  "version": "0.1.0",
  "main": "main.js",
  "license": "MIT",
  "repository": "kitingChris/electron-es6-react",
  "scripts": {
    "start": "electron -r babel-register .",
    "build-package": "electron-packager ./ --out ./build --overwrite",
    "build-css": "node node_modules/node-sass/bin/node-sass --source-map true --source-map-contents --output-style compressed -r -o ./css ./scss"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "electron-packager": "^10.1.1",
    "node-sass": "^4.7.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "electron": "^1.4.3"
  }
}
